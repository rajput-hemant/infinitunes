    "@radix-ui/react-tabs": "^1.0.3",
    "@reduxjs/toolkit": "^1.9.5",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "cva": "^0.0.0",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.1",
    "react-share": "^4.4.1",
