{
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "<TYPES>",
    "<TYPES>^[@~]/(?!$).*$",
    "",
    "^[@~]/.*$",
    "^[.]"
  ],
  "experimentalTernaries": true,
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
